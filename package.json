{
  "name": "@fz6m/eslint-plugin-sakina",
  "version": "2.0.2",
  "description": "Sakina eslint config rules for quick use",
  "author": "fz6m",
  "main": "lib/index.js",
  "homepage": "https://github.com/xn-sakina/eslint-plugin-sakina#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/xn-sakina/eslint-plugin-sakina"
  },
  "files": [
    "lib",
    "setup.js"
  ],
  "bin": {
    "sakina-lint": "bin/sakina-lint.js"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sakina",
    "prettier",
    "eslint-prettier",
    "eslint-react"
  ],
  "scripts": {
    "test": "pnpm clean:output && node test/index.js",
    "clean:output": "rimraf test/output",
    "clean": "rimraf lib",
    "build": "pnpm clean && tsc",
    "prepublishOnly": "pnpm build",
    "release": "npm publish --registry=https://registry.npmjs.com/",
    "test:init": "node bin/sakina-lint.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.6",
    "commit-verify": "^1.0.3",
    "eslint": "^8.13.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/parser": "^5.19.0",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "prettier": "^2.6.2",
    "v8-compile-cache": "^2.3.0"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "license": "MIT"
}
